	background-size: 100%;
}
.blog_box {
	margin: 15px 0;
}
.blog_box .blog_img {
}
.blog_box .blog_img img {
	width: 100%;
	border-radius: 9px;
}
.blog_box .blog_txt {
	width: 90%;
	margin: 0 auto;
	margin-top: -25px;
	padding: 15px;
	position: relative;
	background: #fff;
	box-shadow: 0 0 9px #999;
	border-radius: 9px;
}
.blog_box .blog_txt h6 {
	padding: 15px 0 0;
	font-weight: 800;
}
.blog_box .blog_txt p {
	color: #2c2c52;
}
ul.tv {
	width: 100%;
	height: auto;
	display: table;
}
ul.tv li {
	width: auto;
	height: auto;
	float: left;
	font: 700 10px/20px 'Lato', sans-serif;
	color: #9fa3a7;
	padding: 0 5px 0 20px;
	text-transform: uppercase;
}
ul.tv li:last-child {
	float: right;
}
ul.tv li.time {
	background: url(../images/time.png) no-repeat center left;
}
ul.tv li.comment {
	background: url(../images/comment.png) no-repeat center left;
}
ul.tv li.view {
	background: url(../images/view.png) no-repeat center left;
}
#owl-one .item {
	margin: 15px;
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: center;
}
#owl-one .item img {
	display: inline-block;
}
#owl-one .owl-nav [class*=owl-] {
	position: absolute;
	margin-top: -20px;
	outline: 0;
	width: 28px;
	height: 54px;
	text-indent: -9999px;
	z-index: 999;
}
#owl-one .owl-nav button.owl-next {
	background-position: -63px -1px !important;
	right: -50px;
	top: 50%;
	background: url(../images/right.png) no-repeat !important;
	border-radius: 0;
	margin: 0;
}
#owl-one .owl-nav button.owl-prev {
	background-position: -3px -1px !important;
	left: -50px;
	top: 50%;
	background: url(../images/left.png) no-repeat !important;
