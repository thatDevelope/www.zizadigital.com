	font-weight: 800;
}
#owl-one .owl-stage {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: center;
}
.client_area {
	margin: 25px 0 0 0;
}
.client_area ul {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: center;
}
.client_area ul li {
	width: 19%;
